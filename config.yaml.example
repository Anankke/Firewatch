pyrogram:
  api_id:
  api_hash:
  bot_token: null #if null assumes user bot.
  workers: 4
  updates_workers: 4
  lang_code: cn

proxy:
  enabled: false
  hostname: 127.0.0.1
  port: 1080
  username:
  password:

firewatch:
  watch:
    - # dup as many u wish
      chat_id:
        - any # if any all the messages match target_id in sight will be firewatched
      target_id:
        - any # if any all the messages in chat_id will be firewatched
      dest_chat_id: -1000000000000 # the destination chat

